package com.company;

public class Main {

    public static void main(String[] args) {
        String s=new String("abcd只显示中文");
        String b=new String();
        for(int i=0,len=s.length();i<len;i++){
            char t=s.charAt(i);
            String str=String.valueOf(t);
            if(str.getBytes().length==3){
                b+=str;
            }
        }
        System.out.println(b);
    }
}
